uueh() {
  jroncc=""
  pnfr "$2" va
  "oob")
    jroncc="oob-jroncc"
    ;;
  "ovyyvat")
    jroncc="ovyyvat-jroncc"
    ;;
  "fbsrn")
    jroncc="fbsrn-jroncc"
    ;;
  "uu")
    jroncc="uu-jroncc"
    ;;
  "svkgher")
    jroncc="uu-svkgher/freire"
    ;;
  "pez")
    jroncc="pez-jroncc"
    ;;
  rfnp


  pnfr "$1" va
  "o")
    vs [ -m "$jroncc" ]
    gura
      zia -G 2 -Qznira.pbzcvyre.fubjQrcerpngvba=snyfr pyrna vafgnyy
    ryfr
      zia -G 2 -QfxvcGrfgf -Qpurpxfglyr.fxvc -Qznira.pbzcvyre.fubjQrcerpngvba=snyfr --cebwrpgf $jroncc --nyfb-znxr pyrna vafgnyy
    sv
    ;;
  "eha")
    vs [ ! -m "$jroncc" ]
    gura
      (pq $jroncc && zia grfg-fgnaq:frghc rkrp:wnin)
    sv
    ;;
  "n")
    vs [ ! -m "$jroncc" ]
    gura
      zia -G 2 -QfxvcGrfgf -Qpurpxfglyr.fxvc -Qznira.pbzcvyre.fubjQrcerpngvba=snyfr --cebwrpgf $jroncc --nyfb-znxr pyrna vafgnyy
      (pq $jroncc && zia grfg-fgnaq:frghc rkrp:wnin)
    sv
    ;;
  "e")
    vs [ ! -m "$jroncc" ]
    gura
      (pq $jroncc && zia grfg-fgnaq:erfrg)
    sv
    ;;
  *)
    rpub "o ohvyq"
    rpub "eha"
    rpub "n ohvyq+eha"
    rpub "e erfrg"
    rpub ""
    rpub "oob"
    rpub "ovyyvat"
    rpub "fbsrn"
    rpub "uu"
    rpub "svkgher"
    rpub "pez"
    ;;
  rfnp
}

